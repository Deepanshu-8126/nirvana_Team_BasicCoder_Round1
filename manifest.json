{
  "manifest_version": 3,
  "name": "Basic Coder Marks",
  "version": "1.0",
  "description": "Simple, human-made bookmark manager",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Basic Coder Marks"
  },
  "icons": {
    "16": "icons8-bookmark-16 (1).png",
    "48": "icons8-bookmark-48.png",
    "128": "icons8-bookmark-128.png"
  }
}